msgstr "Correo"

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_EMAIL_HELP_TEXT"
msgstr "Escriba el correo con el cual suscribio su cuenta."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_CODE_CONFIRMATION"
msgstr "Confirmación de código"

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_CODE_CONFIRMATION_HELP_TEXT"
msgstr "Introduzca el código que se envió a su dirección de correo electrónico."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_PASSWORD"
msgstr "Contraseña"

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_PASSWORD_CONFIRMATION"
msgstr "Confirmación de contraseña"

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_VALIDATION This email address is not subscribed to any account."
msgstr "Esta dirección de correo electrónico no está suscrito a ninguna cuenta."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_VALIDATION In a few seconds you can try to recover your account."
msgstr "En unos segundos puedes intentar recuperar su cuenta."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_VALIDATION It is been %(minutes)s minutes since you tried to recover your account, you should wait about %(minutes_max)s minutes."
msgstr "Han pasado %(minutes)s minutos desde que intentaste recuperar tu cuenta, debes esperar unos %(minutes_max)s minutos."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_VALIDATION This code is not correct."
msgstr "Este código no es correcto."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_VALIDATION The password and your confirmation do not match."
msgstr "La contraseña y su confirmación no coinciden."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_MESSAGE This window should not close and only works %(minutes_max)s minutes after sending the confirmation code to your email address."
msgstr "Esta ventana no debe cerrarla y sólo funciona %(minutes_max)s minutos después de enviar el código de confirmación a su dirección de correo electrónico."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_MESSAGE This window should not close and only works %(minutes_max)s minutes after sending the confirmation code to your email address that was %(minutes)s minutes ago."
msgstr "Esta ventana no debe cerrarla y sólo funciona %(minutes_max)s minutos después de enviar el código de confirmación a su dirección de correo electrónico que fue hace %(minutes)s minutos."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_MESSAGE An email has been sent to %(email)s with a verification code that will be entered in the field called code confirmation."
msgstr "Se ha enviado un correo electrónico a <strong>&OpenCurlyDoubleQuote;%(email)s&CloseCurlyDoubleQuote;</strong> con un código de verificación que se ingresará en el campo denominado <strong>&OpenCurlyDoubleQuote;confirmación de código&CloseCurlyDoubleQuote;</strong>."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_MESSAGE Now you can change your password to access the application."
msgstr "Ahora usted puede cambiar su contraseña para poder acceder a la aplicación."

msgid "SECURITY_LOGIN_FORGOT_CREDENTIAL_MESSAGE Try to access the application."
msgstr "Intente acceder a la aplicación."



